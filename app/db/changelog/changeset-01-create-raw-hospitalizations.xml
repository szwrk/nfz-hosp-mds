            <column name="plec_pacjenta" type="VARCHAR(50)"/>
            <column name="grupa_wiekowa_pacjenta" type="VARCHAR(50)"/>
            <column name="przedzial_dlugosci_trwania_hospitalizacji" type="VARCHAR(50)"/>
